<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<BorderPane prefHeight="500.0" prefWidth="583.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171">
   <center>
      <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="tableView" prefHeight="472.0" prefWidth="447.0">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </TableView>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </center>
   <top>
      <Separator prefHeight="13.0" prefWidth="583.0" BorderPane.alignment="CENTER" />
   </top>
   <left>
      <VBox prefHeight="487.0" prefWidth="137.0" spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="top10BooksBtn" mnemonicParsing="false" onAction="#showTop10BooksListener" prefHeight="69.0" prefWidth="141.0" text="Show Top 10 Books" />
            <Button fx:id="top5UserBtn" mnemonicParsing="false" onAction="#showTop5UserListener" prefHeight="69.0" prefWidth="141.0" text="Show Top 5 Users" />
            <Button fx:id="totalSalesBtn" mnemonicParsing="false" onAction="#showTotalSalesListener" prefHeight="69.0" prefWidth="141.0" text="Show Total Sales" />
            <Label fx:id="totalSales" prefHeight="80.0" prefWidth="124.0" />
            <Label fx:id="errorLabel" prefHeight="83.0" prefWidth="123.0" />
         </children>
         <padding>
            <Insets left="10.0" top="50.0" />
         </padding>
      </VBox>
   </left>
</BorderPane>
